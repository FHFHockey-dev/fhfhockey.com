{
	"version": 2,
			"functions": {
				"api/**/*.py": {
					"runtime": "python3.11",
					"maxDuration": 240,
					"memory": 1024
				}
			},
	"routes": [
		{ "src": "^/sko/pipeline$", "dest": "/api/index.py" },
		{ "src": "^/api/(.*)", "dest": "/api/$1" },
		{ "src": "^(?!/api/).*", "dest": "/api/index.py" }
	]
}
