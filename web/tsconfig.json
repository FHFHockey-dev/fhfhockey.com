{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "vitest.config.mts", "lib/supabase/GoaliePage/calculateAverages.js", "components/TeamLandingPage/teamLandingPage.js"],
  "exclude": ["node_modules"]
}
